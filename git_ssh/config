Match host=gitlab.com
   User git
   Hostname gitlab.com
   IdentityFile ~/.ssh/github
   TCPKeepAlive yes
   IdentitiesOnly yes
   AddKeysToAgent yes

Match host=github.com
   User git
   Hostname github.com
   IdentityFile ~/.ssh/github
   TCPKeepAlive yes
   IdentitiesOnly yes
   AddKeysToAgent yes

